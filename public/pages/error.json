{
  "type": "page",
  "data": {
    "brandName": "海量ADP",
    "logoSrc": "/img/logo.png",
    "title": "系统错误",
    "errorMsg": "抱歉，未能找到这个网址，请联系管理员！"
  },
  "style": {
    "backcolor": "white",
    "background": "url('/img/login_bg.png') no-repeat center 0px",
    "width": "100%",
    "height": "100%"
  },
  "body": {
    "type": "container",
    "style": {
      "position": "relative",
      "top": "100px",
      "left": "calc((100% - 400px)/2)",
      "width": "400px"
    },
    "body":[
      {
        "type": "tpl",
        "tpl": "<img src='/img/logo.png' class='mr-2' style='width:30px;height:30px;'/>${brandName}",
        "className": "text-info text-lg text-center w-full inline-block mb-5"
      },
      {
        "type": "form",
        "title": "${title}",
        "submitText": "",
        "wrapWithPanel": true,
        "mode": "horizontal",
        "controls": [
          {
            "type": "tpl",
            "tpl": "${errorMsg}",
            "className": "text-gray-400 text-base text-center w-full inline-block mb-5"
          }
        ]
      }
    ]
  }
}