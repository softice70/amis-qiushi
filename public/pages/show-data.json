{
  "type": "page",
  "body": [
    {
      "type": "hbox",
      "className": "border border-solid rounded border-gray-300 shadow-md",
      "columns": [
        {
          "type": "wrapper",
          "className": "relative",
          "body": [
            {
              "type": "icon",
              "className": "fa fa-newspaper-o text-lg text-info"
            },
            {
              "type": "tpl",
              "tpl": "光大证券-负面标签",
              "inline": true,
              "className": "text-lg font-normal ml-2"
            }
          ]
        },
        {
          "type": "wrapper",
          "body": [
            {
              "type": "button",
              "label": " ",
              "actionType": "dialog",
              "dialog": {
                "title": "主题配置",
                "actions": [
                  {
                    "label": "取消",
                    "actionType": "cancel",
                    "type": "button",
                    "level": "default"
                  },
                  {
                    "label": "保存",
                    "actionType": "submit",
                    "primary": true,
                    "type": "button"
                  }
                ],
                "body": {
                  "type": "form",
                  "initApi": "get:/api/getThemeCfg?id=${id}",
                  "api": "post:/api/saveThemeCfg?id=${id}",
                  "controls": [
                    {
                      "name": "showFields",
                      "type": "checkboxes",
                      "label": "展示字段",
                      "joinValues": true,
                      "columnsCount": 3,
                      "options": [
                        {
                          "label": "时间",
                          "value": "time"
                        },
                        {
                          "label": "来源",
                          "value": "mediaName"
                        },
                        {
                          "label": "出处",
                          "value": "contentMediaName"
                        },
                        {
                          "label": "资源类型",
                          "value": "sourceType"
                        },
                        {
                          "label": "正负情感",
                          "value": "relativity"
                        },
                        {
                          "label": "摘要",
                          "value": "summary"
                        }
                      ]
                    },
                    {
                      "type": "group",
                      "controls": [
                        {
                          "label": "发布时间",
                          "type": "select",
                          "name": "timeFieldName",
                          "multiple": false,
                          "options": [
                            {
                              "label": "发布时间",
                              "value": "release_date"
                            },
                            {
                              "label": "采集时间",
                              "value": "download_date"
                            },
                            {
                              "label": "入库时间",
                              "value": "add_time"
                            }
                          ]
                        },
                        {
                          "label": "时间排序",
                          "type": "select",
                          "name": "sortType",
                          "multiple": false,
                          "options": [
                            {
                              "label": "升序",
                              "value": "asc"
                            },
                            {
                              "label": "降序",
                              "value": "desc"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "group",
                      "controls": [
                        {
                          "label": "消重算法",
                          "type": "select",
                          "name": "DeduplicatedBy",
                          "multiple": false,
                          "options": [
                            {
                              "label": "内容相似消重",
                              "value": "content"
                            },
                            {
                              "label": "标题相似消重",
                              "value": "title"
                            }
                          ]
                        },
                        {
                          "label": "预览方式",
                          "type": "select",
                          "name": "showLink",
                          "multiple": false,
                          "options": [
                            {
                              "label": "页面弹出层",
                              "value": "inDrawer"
                            },
                            {
                              "label": "新标签页",
                              "value": "inBlank"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              },
              "className": "pull-right no-border text-info text-xl p-none",
              "icon": "fa fa-cog",
              "size": "md"
            }
          ]
        }
      ]
    },
    {
      "type": "tabs",
      "tabs": [
        {
          "title": "数据视图",
          "body": [
            {
              "type": "service",
              "data": {
                "dataView": "themeData",
                "dict":{
                  "relativity": {
                    "0": "中",
                    "-1": "负",
                    "1": "正"
                  },
                  "source_type": {
                    "0": "资讯",
                    "1": "论坛",
                    "2": "贴吧",
                    "3": "博客",
                    "4": "微博",
                    "11": "微信"
                  }
                }
              },
              "schemaApi": "get:/pages/show-article-list.json"
            }
          ],
          "icon": "fa fa-list-alt"
        },
        {
          "title": "无效数据",
          "body": [
            {
              "type": "service",
              "data": {
                "dataView": "invalidData",
                "dict":{
                  "relativity": {
                    "0": "中",
                    "-1": "负",
                    "1": "正"
                  },
                  "source_type": {
                    "0": "资讯",
                    "1": "论坛",
                    "2": "贴吧",
                    "3": "博客",
                    "4": "微博",
                    "11": "微信"
                  }
                }
              },
              "schemaApi": "get:/pages/show-article-list.json"
            }
          ],
          "icon": "fa fa-trash-o"
        }
      ],
      "tabsMode": "line"
    }
  ],
  "className": "bg-white"
}